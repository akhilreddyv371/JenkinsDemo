@Library("AutomateSharedLibrary") _
pipeline{
  agent any
  stages{
    stage("hello"){
      steps{
        script{
          hello()
        }
      }
    }
     stage("develop"){
      steps{
        script{
          develop()
        }
      }
    }
     stage("build"){
      steps{
        script{
          build()
        }
      }
    }
     stage("test"){
      steps{
        script{
          test()
        }
      }
    }
    stage("load config files")
    {
      steps{
        script{
          loadConfig("config.json")
        }
      }
    }
     stage("deploy"){
      steps{
        script{
          deploy()
        }
      }
    }
    stage("end"){
      steps{
        script{
          end()
        }
      }
    }
  }
}
